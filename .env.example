NODE_ENV=development

# Backend / API
PORT=4000
FRONTEND_URL=http://localhost:5173
# Utiliser une valeur forte et unique en production
JWT_SECRET=YOUR_JWT_SECRET
# 32 bytes en base64 ou hex (ex: `openssl rand -base64 32`)
DATA_ENCRYPTION_KEY=YOUR_32_BYTE_KEY_BASE64_OR_HEX
TOTP_ISSUER=BTPGo

# Base de données (Supabase recommandé pour prod)
# Exemple: postgresql://postgres:<PASSWORD>@db.<PROJECT>.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=

# Cache / Queue
REDIS_URL=redis://localhost:6379

# Frontend
VITE_BACKEND_URL=https://api.msmarrakech.com

# IA / Clés externes (optionnel)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Billing (Stripe)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_STARTER=
STRIPE_PRICE_PRO=
BILLING_SUCCESS_URL=
BILLING_CANCEL_URL=

# Emails (optionnel)
EMAIL_SERVER=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=
EMAIL_PASSWORD=

# Observability (OpenTelemetry)
# Exemple d'endpoint OTLP HTTP local (Tempo/Collector) : http://localhost:4318
OTEL_EXPORTER_OTLP_ENDPOINT=
OTEL_SERVICE_NAME=

# SSO OIDC (Auth0 / Azure AD / Generic OIDC)
OIDC_ISSUER=
OIDC_CLIENT_ID=
OIDC_CLIENT_SECRET=
OIDC_REDIRECT_URI=


